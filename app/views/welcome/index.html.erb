<% unless current_user %>
  <%= button_to "Create New User", register_path, method: :get %>
  <%= link_to "Log In", login_path %>
<% else %>
  <div>Logged in as <%= current_user.name %></div>
  <div>Location: <%= cookies[:location] %></div>
<% end %>

<% if current_user %>
  <section class='existing-users'>
      <p>Existing Users: </p>
      <% @users.each do |user| %>
          <li><%= link_to user.email, user_path(user.id) %></li>
      <% end %>
  </section>
<% end %>